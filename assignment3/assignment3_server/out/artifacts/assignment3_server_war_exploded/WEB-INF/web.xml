<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

  <servlet>
    <servlet-name>SwipeServlet</servlet-name>
    <servlet-class>SwipeServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>SwipeServlet</servlet-name>
    <url-pattern>/swipe/*</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>StatsServlet</servlet-name>
    <servlet-class>StatsServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>StatsServlet</servlet-name>
    <url-pattern>/stats/*</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>MatchesServlet</servlet-name>
    <servlet-class>MatchesServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>MatchesServlet</servlet-name>
    <url-pattern>/matches/*</url-pattern>
  </servlet-mapping>
</web-app>